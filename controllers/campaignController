const express = require('express');
const router = express.Router();

router.get('/',async (req,res) => {
    
  res.render('index');
});

router.get('/campaign',async (req,res) => {
    
  res.render('campaign');
});

router.get('/campaigndetail',async (req,res) => {
    
  res.render('campaignDetail');
});
module.exports = router;
